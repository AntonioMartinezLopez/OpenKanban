{
    "eslint.validate": [
      "javascript",
      "typescript",
      "vue"
    ],
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true,
    },
    "css.lint.unknownAtRules": "ignore",
    "tailwindCSS.experimental.configFile": "packages/frontend/.nuxt/tailwind.config.cjs",
    "files.associations": {
      "*.css": "tailwindcss"
    },
    "editor.quickSuggestions": {
      "strings": true
    }
  }